{
    "version": 2,
    "builds": [
        {
            "src": "*.php",
            "use": "@vercel/php"
        },
        {
            "src": "mahasiswa/*.php",
            "use": "@vercel/php"
        },
        {
            "src": "dosen/*.php",
            "use": "@vercel/php"
        },
        {
            "src": "config/*.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}